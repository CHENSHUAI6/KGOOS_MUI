<UserControl x:Class="KGOOS_MUI.Pages.Scan.ScanManage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="800">
    <Grid Style="{StaticResource ContentRoot}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition  Width="*" />
            <ColumnDefinition  Width="1" />
            <ColumnDefinition  Width="*" />
        </Grid.ColumnDefinitions>
        <!--<Image Name="image1" Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Top"
               Width="200" Height="200" Source="/Img/1.jpg" MouseDown="Image_MouseDown">
        </Image>-->
        <TextBlock Name="Weigh" Text="到件称重扫描" FontSize="30" FontWeight="Bold" HorizontalAlignment="Center" 
                   VerticalAlignment="Top" FontFamily="微软雅黑" MouseDown="Weigh_MouseDown" />


        <TextBlock Name="Inquire" Grid.Column="2" Text="到件扫描查询" FontSize="30" FontWeight="Bold" 
                   HorizontalAlignment="Center" VerticalAlignment="Top" FontFamily="微软雅黑" MouseDown="Inquire_MouseDown" />
   
    </Grid>
</UserControl>
